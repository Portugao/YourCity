{# purpose of this template: parts of menu xml inclusion template #}
<partofmenu id="{{ partOfMenu.getId() }}" createdon="{{ partOfMenu.createdDate|localizeddate('medium', 'short') }}" updatedon="{{ partOfMenu.updatedDate|localizeddate('medium', 'short') }}">
    <id>{{ partOfMenu.id }}</id>
    <name><![CDATA[{{ partOfMenu.name }}]]></name>
    <description><![CDATA[{{ partOfMenu.description|safeHtml }}]]></description>
    <positionOfPart>{{ partOfMenu.positionOfPart }}</positionOfPart>
    <workflowState>{{ partOfMenu.workflowState|muyourcitymodule_objectState(false)|lower }}</workflowState>
    <menuOfLocation>{% if partOfMenu.menuOfLocation|default %}{{ partOfMenu.menuOfLocation|muyourcitymodule_formattedTitle }}{% endif %}</menuOfLocation>
    <dishes>
    {% if partOfMenu.dishes|default %}
        {% for relatedItem in partOfMenu.dishes %}
        <dish>{{ relatedItem|muyourcitymodule_formattedTitle }}</dish>
        {% endfor %}
    {% endif %}
    </dishes>
</partofmenu>
