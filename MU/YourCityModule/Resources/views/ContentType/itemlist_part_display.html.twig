{# Purpose of this template: Display parts within an external context #}
{% for part in items %}
    <h3>{{ part|muyourcitymodule_formattedTitle }}</h3>
    <p><a href="{{ path('muyourcitymodule_part_display', {'id': part.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
