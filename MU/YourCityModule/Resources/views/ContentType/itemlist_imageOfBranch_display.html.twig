{# Purpose of this template: Display images of branch within an external context #}
{% for imageOfBranch in items %}
    <h3>{{ imageOfBranch.getTitleFromDisplayPattern() }}</h3>
    <p><a href="{{ path('muyourcitymodule_imageofbranch_display', { 'id': imageOfBranch.id }) }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
