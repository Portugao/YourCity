{# Purpose of this template: Display images of location within an external context #}
{% for imageOfLocation in items %}
    <h3>{{ imageOfLocation|muyourcitymodule_formattedTitle }}</h3>
    <p><a href="{{ path('muyourcitymodule_imageoflocation_display', { 'id': imageOfLocation.id }) }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
