{# Purpose of this template: Display services of location within an external context #}
{% for serviceOfLocation in items %}
    <h3>{{ serviceOfLocation|muyourcitymodule_formattedTitle }}</h3>
    <p><a href="{{ path('muyourcitymodule_serviceoflocation_display', {'id': serviceOfLocation.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
