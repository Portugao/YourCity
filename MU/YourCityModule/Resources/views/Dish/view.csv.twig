{# purpose of this template: dishes view csv view #}
{% spaceless %}"{{ __('Name') }}";"{{ __('Description') }}";"{{ __('Kind of dish') }}";"{{ __('Image of dish') }}";"{{ __('Price of dish') }}";"{{ __('Ingredients') }}";"{{ __('Position of dish') }}";"{{ __('My location') }}";"{{ __('Workflow state') }}"
