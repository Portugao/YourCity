{# purpose of this template: locations xml inclusion template #}
<location id="{{ location.getId() }}" createdon="{{ location.createdDate|localizeddate('medium', 'short') }}" updatedon="{{ location.updatedDate|localizeddate('medium', 'short') }}">
    <id>{{ location.id }}</id>
    <name><![CDATA[{{ location.name }}]]></name>
    <slogan><![CDATA[{{ location.slogan }}]]></slogan>
    <letterForOrder><![CDATA[{{ location.letterForOrder }}]]></letterForOrder>
    <keywordsForLocation><![CDATA[{{ location.keywordsForLocation }}]]></keywordsForLocation>
    <descriptionForGoogle><![CDATA[{{ location.descriptionForGoogle }}]]></descriptionForGoogle>
    <logoOfYourLocation{% if location.logoOfYourLocation is not empty and location.logoOfYourLocationMeta|default %} extension="{{ location.logoOfYourLocationMeta.extension }}" size="{{ location.logoOfYourLocationMeta.size }}" isImage="{% if location.logoOfYourLocationMeta.isImage %}true{% else %}false{% endif %}"{% if location.logoOfYourLocationMeta.isImage %} width="{{ location.logoOfYourLocationMeta.width }}" height="{{ location.logoOfYourLocationMeta.height }}" format="{{ location.logoOfYourLocationMeta.format }}"{% endif %}{% endif %}>{{ location.logoOfYourLocation }}</logoOfYourLocation>
    <description><![CDATA[{{ location.description|safeHtml }}]]></description>
    <description2><![CDATA[{{ location.description2|safeHtml }}]]></description2>
    <imageOfLocation{% if location.imageOfLocation is not empty and location.imageOfLocationMeta|default %} extension="{{ location.imageOfLocationMeta.extension }}" size="{{ location.imageOfLocationMeta.size }}" isImage="{% if location.imageOfLocationMeta.isImage %}true{% else %}false{% endif %}"{% if location.imageOfLocationMeta.isImage %} width="{{ location.imageOfLocationMeta.width }}" height="{{ location.imageOfLocationMeta.height }}" format="{{ location.imageOfLocationMeta.format }}"{% endif %}{% endif %}>{{ location.imageOfLocation }}</imageOfLocation>
    <street><![CDATA[{{ location.street }}]]></street>
    <numberOfStreet><![CDATA[{{ location.numberOfStreet }}]]></numberOfStreet>
    <zipCode><![CDATA[{{ location.zipCode }}]]></zipCode>
    <city><![CDATA[{{ location.city }}]]></city>
    <partOfCity>{{ location.partOfCity|muyourcitymodule_listEntry('location', 'partOfCity') }}</partOfCity>
    <telefon><![CDATA[{{ location.telefon }}]]></telefon>
    <mobil><![CDATA[{{ location.mobil }}]]></mobil>
    <homepage>{{ location.homepage }}</homepage>
    <bsagStop><![CDATA[{{ location.bsagStop }}]]></bsagStop>
    <tram><![CDATA[{{ location.tram }}]]></tram>
    <bus><![CDATA[{{ location.bus }}]]></bus>
    <closedForEver>{% if not location.closedForEver %}0{% else %}1{% endif %}</closedForEver>
    <agreement>{% if not location.agreement %}0{% else %}1{% endif %}</agreement>
    <unclearTimes>{% if not location.unclearTimes %}0{% else %}1{% endif %}</unclearTimes>
    <openingHours><![CDATA[{{ location.openingHours|safeHtml }}]]></openingHours>
    <closedOnMonday>{% if not location.closedOnMonday %}0{% else %}1{% endif %}</closedOnMonday>
    <startOnMonday>{{ location.startOnMonday|localizeddate('none', 'short') }}</startOnMonday>
    <endOnMonday>{{ location.endOnMonday|localizeddate('none', 'short') }}</endOnMonday>
    <start2OnMonday>{{ location.start2OnMonday|localizeddate('none', 'short') }}</start2OnMonday>
    <end2OnMonday>{{ location.end2OnMonday|localizeddate('none', 'short') }}</end2OnMonday>
    <closedOnTuesday>{% if not location.closedOnTuesday %}0{% else %}1{% endif %}</closedOnTuesday>
    <startOnTuesday>{{ location.startOnTuesday|localizeddate('none', 'short') }}</startOnTuesday>
    <endOnTuesday>{{ location.endOnTuesday|localizeddate('none', 'short') }}</endOnTuesday>
    <start2OnTuesday>{{ location.start2OnTuesday|localizeddate('none', 'short') }}</start2OnTuesday>
    <end2OnTuesday>{{ location.end2OnTuesday|localizeddate('none', 'short') }}</end2OnTuesday>
    <closedOnWednesday>{% if not location.closedOnWednesday %}0{% else %}1{% endif %}</closedOnWednesday>
    <startOnWednesday>{{ location.startOnWednesday|localizeddate('none', 'short') }}</startOnWednesday>
    <endOnWednesday>{{ location.endOnWednesday|localizeddate('none', 'short') }}</endOnWednesday>
    <start2OnWednesday>{{ location.start2OnWednesday|localizeddate('none', 'short') }}</start2OnWednesday>
    <end2OnWednesday>{{ location.end2OnWednesday|localizeddate('none', 'short') }}</end2OnWednesday>
    <closedOnThursday>{% if not location.closedOnThursday %}0{% else %}1{% endif %}</closedOnThursday>
    <startOnThursday>{{ location.startOnThursday|localizeddate('none', 'short') }}</startOnThursday>
    <endOnThursday>{{ location.endOnThursday|localizeddate('none', 'short') }}</endOnThursday>
    <start2OnThursday>{{ location.start2OnThursday|localizeddate('none', 'short') }}</start2OnThursday>
    <end2OnThursday>{{ location.end2OnThursday|localizeddate('none', 'short') }}</end2OnThursday>
    <closedOnFriday>{% if not location.closedOnFriday %}0{% else %}1{% endif %}</closedOnFriday>
    <startOnFriday>{{ location.startOnFriday|localizeddate('none', 'short') }}</startOnFriday>
    <endOnFriday>{{ location.endOnFriday|localizeddate('none', 'short') }}</endOnFriday>
    <start2OnFriday>{{ location.start2OnFriday|localizeddate('none', 'short') }}</start2OnFriday>
    <end2OnFriday>{{ location.end2OnFriday|localizeddate('none', 'short') }}</end2OnFriday>
    <closedOnSaturday>{% if not location.closedOnSaturday %}0{% else %}1{% endif %}</closedOnSaturday>
    <startOnSaturday>{{ location.startOnSaturday|localizeddate('none', 'short') }}</startOnSaturday>
    <endOnSaturday>{{ location.endOnSaturday|localizeddate('none', 'short') }}</endOnSaturday>
    <start2OnSaturday>{{ location.start2OnSaturday|localizeddate('none', 'short') }}</start2OnSaturday>
    <end2OnSaturday>{{ location.end2OnSaturday|localizeddate('none', 'short') }}</end2OnSaturday>
    <closedOnSunday>{% if not location.closedOnSunday %}0{% else %}1{% endif %}</closedOnSunday>
    <startOnSunday>{{ location.startOnSunday|localizeddate('none', 'short') }}</startOnSunday>
    <endOnSunday>{{ location.endOnSunday|localizeddate('none', 'short') }}</endOnSunday>
    <start2OnSunday>{{ location.start2OnSunday|localizeddate('none', 'short') }}</start2OnSunday>
    <end2OnSunday>{{ location.end2OnSunday|localizeddate('none', 'short') }}</end2OnSunday>
    <firstImage{% if location.firstImage is not empty and location.firstImageMeta|default %} extension="{{ location.firstImageMeta.extension }}" size="{{ location.firstImageMeta.size }}" isImage="{% if location.firstImageMeta.isImage %}true{% else %}false{% endif %}"{% if location.firstImageMeta.isImage %} width="{{ location.firstImageMeta.width }}" height="{{ location.firstImageMeta.height }}" format="{{ location.firstImageMeta.format }}"{% endif %}{% endif %}>{{ location.firstImage }}</firstImage>
    <secondImage{% if location.secondImage is not empty and location.secondImageMeta|default %} extension="{{ location.secondImageMeta.extension }}" size="{{ location.secondImageMeta.size }}" isImage="{% if location.secondImageMeta.isImage %}true{% else %}false{% endif %}"{% if location.secondImageMeta.isImage %} width="{{ location.secondImageMeta.width }}" height="{{ location.secondImageMeta.height }}" format="{{ location.secondImageMeta.format }}"{% endif %}{% endif %}>{{ location.secondImage }}</secondImage>
    <thirdImage{% if location.thirdImage is not empty and location.thirdImageMeta|default %} extension="{{ location.thirdImageMeta.extension }}" size="{{ location.thirdImageMeta.size }}" isImage="{% if location.thirdImageMeta.isImage %}true{% else %}false{% endif %}"{% if location.thirdImageMeta.isImage %} width="{{ location.thirdImageMeta.width }}" height="{{ location.thirdImageMeta.height }}" format="{{ location.thirdImageMeta.format }}"{% endif %}{% endif %}>{{ location.thirdImage }}</thirdImage>
    <fourthImage{% if location.fourthImage is not empty and location.fourthImageMeta|default %} extension="{{ location.fourthImageMeta.extension }}" size="{{ location.fourthImageMeta.size }}" isImage="{% if location.fourthImageMeta.isImage %}true{% else %}false{% endif %}"{% if location.fourthImageMeta.isImage %} width="{{ location.fourthImageMeta.width }}" height="{{ location.fourthImageMeta.height }}" format="{{ location.fourthImageMeta.format }}"{% endif %}{% endif %}>{{ location.fourthImage }}</fourthImage>
    <fifthImage{% if location.fifthImage is not empty and location.fifthImageMeta|default %} extension="{{ location.fifthImageMeta.extension }}" size="{{ location.fifthImageMeta.size }}" isImage="{% if location.fifthImageMeta.isImage %}true{% else %}false{% endif %}"{% if location.fifthImageMeta.isImage %} width="{{ location.fifthImageMeta.width }}" height="{{ location.fifthImageMeta.height }}" format="{{ location.fifthImageMeta.format }}"{% endif %}{% endif %}>{{ location.fifthImage }}</fifthImage>
    <sixthImage{% if location.sixthImage is not empty and location.sixthImageMeta|default %} extension="{{ location.sixthImageMeta.extension }}" size="{{ location.sixthImageMeta.size }}" isImage="{% if location.sixthImageMeta.isImage %}true{% else %}false{% endif %}"{% if location.sixthImageMeta.isImage %} width="{{ location.sixthImageMeta.width }}" height="{{ location.sixthImageMeta.height }}" format="{{ location.sixthImageMeta.format }}"{% endif %}{% endif %}>{{ location.sixthImage }}</sixthImage>
    <firstFile{% if location.firstFile is not empty and location.firstFileMeta|default %} extension="{{ location.firstFileMeta.extension }}" size="{{ location.firstFileMeta.size }}" isImage="{% if location.firstFileMeta.isImage %}true{% else %}false{% endif %}"{% if location.firstFileMeta.isImage %} width="{{ location.firstFileMeta.width }}" height="{{ location.firstFileMeta.height }}" format="{{ location.firstFileMeta.format }}"{% endif %}{% endif %}>{{ location.firstFile }}</firstFile>
    <secondFile{% if location.secondFile is not empty and location.secondFileMeta|default %} extension="{{ location.secondFileMeta.extension }}" size="{{ location.secondFileMeta.size }}" isImage="{% if location.secondFileMeta.isImage %}true{% else %}false{% endif %}"{% if location.secondFileMeta.isImage %} width="{{ location.secondFileMeta.width }}" height="{{ location.secondFileMeta.height }}" format="{{ location.secondFileMeta.format }}"{% endif %}{% endif %}>{{ location.secondFile }}</secondFile>
    <owner>{% if location.owner|default and location.owner.getUid() > 0 %}{{ location.owner.getUname() }}{% endif %}</owner>
    <admin1>{% if location.admin1|default and location.admin1.getUid() > 0 %}{{ location.admin1.getUname() }}{% endif %}</admin1>
    <admin2>{% if location.admin2|default and location.admin2.getUid() > 0 %}{{ location.admin2.getUname() }}{% endif %}</admin2>
    <latitude>{{ location.latitude|muyourcitymodule_geoData }}</latitude>
    <longitude>{{ location.longitude|muyourcitymodule_geoData }}</longitude>
    <workflowState>{{ location.workflowState|muyourcitymodule_objectState(false)|lower }}</workflowState>
    <abonnements>
    {% if location.abonnements|default %}
        {% for relatedItem in location.abonnements %}
        <abonnement>{{ relatedItem|muyourcitymodule_formattedTitle }}</abonnement>
        {% endfor %}
    {% endif %}
    </abonnements>
    <branches>
    {% if location.branches|default %}
        {% for relatedItem in location.branches %}
        <branch>{{ relatedItem|muyourcitymodule_formattedTitle }}</branch>
        {% endfor %}
    {% endif %}
    </branches>
    <servicesOfLocation>
    {% if location.servicesOfLocation|default %}
        {% for relatedItem in location.servicesOfLocation %}
        <serviceOfLocation>{{ relatedItem|muyourcitymodule_formattedTitle }}</serviceOfLocation>
        {% endfor %}
    {% endif %}
    </servicesOfLocation>
    <specialsOfLocation>
    {% if location.specialsOfLocation|default %}
        {% for relatedItem in location.specialsOfLocation %}
        <specialOfLocation>{{ relatedItem|muyourcitymodule_formattedTitle }}</specialOfLocation>
        {% endfor %}
    {% endif %}
    </specialsOfLocation>
</location>
