    <div class="row">
        <div class="col-sm-9">
    <dl>
        {% if location.workflowState is not empty and routeArea == 'admin' %}
            <dt>{{ __('State') }}</dt>
            <dd>{{ location.workflowState|muyourcitymodule_listEntry('location', 'workflowState') }}</dd>
        {% endif %}
        {% if location.name is not empty %}
            <dt>{{ __('Name') }}</dt>
            <dd>{{ location.name }}</dd>
        {% endif %}
        {% if location.logoOfYourLocation is not empty %}
            <dt>{{ __('Logo of your location') }}</dt>
